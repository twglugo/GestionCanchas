<Window x:Name="Prueba" x:Class="GestinCanchas.ui.Views.ClienteView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestinCanchas.ui.Views"
        mc:Ignorable="d"
        Title="Prueba" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="196,36,196,239">
            <TextBox   x:Name="Cedula"
                       Text ="Cedula" 
                       Margin="0,0,0,10"
                       
            />
            <TextBox   x:Name="Nombre"
                       Text ="Nombre" 
                       Margin="0,0,0,10"
            />
            <TextBox x:Name="Telefono"
                       Text ="Telefono" 
                       Margin="0,0,0,10"
             />

            <Button   x:Name="Prueba1" 
                  Content="Prueba" 
                  BorderBrush="#FF8C3232" 
                  FontFamily="Tahoma" 
                  Foreground="#FF0E0B0B" 
                  Background="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}" RenderTransformOrigin="0.959,-0.943" Height="52" Width="222" Click="Prueba1_Click"/>


        </StackPanel>

        <DataGrid x:Name="Consultatotal" 
                  Margin="102,217,102,24" 
                  AutoGenerateColumns="True"
                  IsReadOnly="True"
                  
            />
    </Grid>
</Window>
